{
  "name": "scrabble-ui",
  "version": "1.0.0",
  "description": "A UI for the game of scrabble",
  "main": "./dist/index.js",
  "scripts": {
    "dev:serve": "node ./node_modules/parcel-bundler/bin/cli serve ./src/public/index.html --out-dir ./dist/public --port 8888",
    "dev:build": "node ./node_modules/parcel-bundler/bin/cli build ./src/public/index.html --out-dir ./dist/public",
    "start": "node ./node_modules/npm-run-all/bin/npm-run-all -p -r dev:serve",
    "lint": "node ./node_modules/eslint/bin/eslint . --ext .jsx,.js --fix"
  },
  "repository": "https://github.com/djroush/scrabble-ui",
  "keywords": [],
  "author": "Doug Roush",
  "license": "MIT",
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/plugin-transform-typescript": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "typescript": "^3.9.6"
  },
  "resolutions": {
    "lodash": "^4.17.19"
  }
}
